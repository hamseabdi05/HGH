{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "nprogress@0.2.0",
      "destination": "wwwroot/lib/nprogress/"
    },
    {
      "library": "font-awesome@6.2.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "jquery@3.6.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "layer@3.5.1",
      "destination": "wwwroot/lib/layer/"
    },
    {
      "library": "iCheck@1.0.3",
      "destination": "wwwroot/lib/iCheck/"
    },
    {
      "library": "bootstrap-daterangepicker@3.0.5",
      "destination": "wwwroot/lib/bootstrap-daterangepicker/"
    },
    {
      "library": "datatables.net-bs4@3.2.2",
      "destination": "wwwroot/lib/datatables.net-bs4/"
    },
    {
      "library": "datatables.net@2.1.1",
      "destination": "wwwroot/lib/datatables.net/"
    },
    {
      "library": "ckeditor@4.20.1",
      "destination": "wwwroot/lib/ckeditor4/plugins/",
      "files": [
        "ckeditor.js",
        "plugins/autogrow/plugin.min.js",
        "plugins/autogrow/plugin.js",
        "plugins/autocomplete/plugin.min.js",
        "plugins/autocomplete/plugin.js",
        "plugins/autocomplete/skins/default.min.css",
        "plugins/autocomplete/skins/default.css"
      ]
    },
    {
      "library": "animate.css@4.1.1",
      "destination": "wwwroot/css/animate.css/"
    },
    {
      "library": "qrcodejs@1.0.0",
      "destination": "wwwroot/lib/qrcodejs/"
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/microsoft/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}